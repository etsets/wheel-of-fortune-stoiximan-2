@model IEnumerable<WheelOfFortune.Models.HistoryEntry>

@{
    ViewData["Title"] = "DepositHistory";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Deposit History</h2>

<table class="table">

    <tr>
        <th>
        </th>
        <th>
            Id
        </th>
        <th>
            Type
        </th>
        <th>
            Time
        </th>
    </tr>

    @foreach (var b in Model)
    {

        <tr>
            <td>
                @Html.ActionLink("Details", "DepositDetails", new { hid = b.HistoryEntryId})
            </td>
            <td>
                @b.HistoryEntryId
            </td>
            <td>
                @b.HistoryEntryTypeId
            </td>
            <td>
                @b.TimeOccurred
            </td>
        </tr>
    }

</table> 